{
  "name_id": "isla_mayor_ndvi_monthly",
  "date_interval": 1,
  "interval_type": "months",
  "download": {
    "enabled": false,
    "url": "https://sh.dataspace.copernicus.eu/api/v1/process",
    "token_url": "https://identity.dataspace.copernicus.eu/auth/realms/CDSE/protocol/openid-connect/token",
    "client_id_env": "COPERNICUS_CLIENT_ID",
    "client_secret_env": "COPERNICUS_CLIENT_SECRET",
    "satellite_type": "sentinel-2-l2a",
    "bbox": [15.959, 48.683, 16.002, 48.71],
    "start_date": "2015-01-01",
    "end_date": "2022-01-01",
    "format": "image/tiff",
    "evalscript": "ndvi",
    "data_filter": {
      "maxCloudCoverage": 10,
      "minCloudCoverage": 0
    }
  },
  "train": {
    "enabled": true,
    "save_visualization": true,
    "save_model": true,
    "start_date": "2015-01-01",
    "end_date": "2022-01-01",
    "kpi": "mean",
    "models": ["arima", "sarima", "random_forest"],
    "model_params": {
      "arima": {},
      "sarima": {
        "seasonal": true,
        "stationary": false,
        "m": 12
      },
      "random_forest": {
        "n_estimators": 100,
        "max_depth": 10
      }
    }
  }
}
